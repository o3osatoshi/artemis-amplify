    "dotenv": "^16.0.3",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "aws-sdk": "^2.1113.0",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.0.0",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  }
